{
  "name": "newtube-backend",
  "version": "1.0.0",
  "description": "NEWTUBE Backend API with GraphQL and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:migrate:init": "tsx src/scripts/migrate.ts -- --init",
    "db:migrate:status": "tsx src/scripts/migrate.ts -- --status",
    "db:migrate:create": "tsx src/scripts/migrate.ts -- --create",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:reset": "tsx src/scripts/reset.ts",
    "db:reset:seed": "tsx src/scripts/reset.ts -- --seed",
    "db:setup": "npm run db:migrate:init && npm run db:seed",
    "codegen": "graphql-codegen --config codegen.ts",
    "test": "jest",
    "test:api": "jest --config jest.integration.config.js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "logs:clean": "rm -rf logs/*.log",
    "logs:create": "mkdir -p logs"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@clerk/backend": "^0.32.0",
    "@prisma/client": "^6.16.0",
    "apollo-server-express": "^3.12.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.20.1",
    "pg": "^8.11.3",
    "prisma": "^6.16.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.4.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "newtube",
    "graphql",
    "postgresql",
    "backend",
    "streaming",
    "aggregator"
  ],
  "author": "NEWTUBE Backend Team",
  "license": "MIT"
}
